<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views ="clr-namespace:FNVCB.VIEW"
                    xmlns:viewModels="clr-namespace:FNVCB.VIEWMODEL;assembly=FNVCB.VIEWMODEL">


<DataTemplate DataType="{x:Type viewModels:Console_VM}">
    <views:Console_View/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:Input_VM}">
        <views:Input_View/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:Guess_VM}">
        <views:Guess_View/>
    </DataTemplate>

</ResourceDictionary>